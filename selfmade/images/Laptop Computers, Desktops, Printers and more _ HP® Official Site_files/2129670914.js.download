    var serachTerms = {
    	'/us/en/pdp/hp-envy-7640-e-all-in-one-printer': '62',
    	'/us/en/pdp/hp-envy-5540-all-in-one-printer': '62',
    	'/us/en/pdp/hp-envy-5660-e-all-in-one-printer': '62',
      '/us/en/pdp/hp-officejet-4650-all-in-one-printer': '63',
      '/us/en/pdp/hp-envy-4520-all-in-one-printer': '63'
    };
    var HPSERVICES = "//store.hp.com/webapp/wcs/stores/servlet/HPServices?langId=-1&storeId=10151&catalogId=10051&_=1459881621410&action=cupis&catentryId=";
	
	var carousalSectionHTML = '<section class="eol-carousal-section"><div class="eol-carousal-container row"><h3>{{title}}</h3><div class="eol-carousal">{{{slides}}}</div></div></section>';
	var carousalSlideHTML = '<div class="eol-carousal-slide"><div class="eol-product-img"><img src="{{imageUrl}}"></div><div class="eol-product-name">{{name}}</div><div class="eol-product-price">${{lPrice}}</div><center><div class="eol-add-to-cart {{#isOOS}}isOOS{{/isOOS}}" data-prodId="{{prodId}}" data-itemId="{{itemId}}" data-sku="{{sku}}" data-regPrice="{{gsPrice}}">{{text}}</div></center></div>';
	// initialize experiment
	function initTest(){
    	var targetBtn = $('.pdpPrice._desktop_only .inventory_holder .btn.bluebtn.text-center');
    	var targetMBtn = $('.priceHolderMbl._mobile_only .inventory_holder .btn.bluebtn.text-center');
		eolTestAddCTA(targetBtn, targetMBtn);
		$('body').on('click', '.inventory_holder a.btn.bluebtn.eol-btn.b-btn', function(){
		    var omni = METRIC_VAR + ':' + window.sku;
		    hpUtil.trackEvent({prefix: METRIC_PREFIX, omni: omni, base: 'shop-replacement-click'});
		});
		$('body').on('click', '.inventory_holder a.btn.bluebtn.eol-btn.i-btn', function(){
		    var omni = METRIC_VAR + ':' + window.sku;
		    hpUtil.trackEvent({prefix: METRIC_PREFIX, omni: omni, base: 'shop-ink-click'});
		});
  	};

	function addInksModule() {
		// get existing service data
	    var serviceData = $.hpsfData.getSvcData();
	    // get product group needed for carousal
	    var productFamily = serachTerms[window.location.pathname];
	    var productGroupName = 'product:' + productFamily + ':family';
	    wd_hpservice.productGroup.get(productGroupName).then(function(data) { 
	    	inks = data.productsList; 
        updateInkBundleName();
	    	var missingProds = findMissingProds(inks, serviceData.priceData);

	    	if ( !!missingProds.length ) {
	    		getProductsData(missingProds, function() {
	    			processData();
	    			createCarousal();
	    		});
	    	} else {
	    		processData();
	    		createCarousal();
	    	}
	    });
	}
  
  function updateInkBundleName() {
  	inks.forEach(function(ink) {
    	if ( ink.name.indexOf('Bundle') >= 0 ) {
      	ink.name = ink.name.replace('Ink Cartridge ', '');
      }
    });
  }

	function processData() {
		var serviceData = $.hpsfData.getSvcData();
	    var prices = serviceData.priceData.reduce(function(a,c) {
	  		a[c.productId] = {
				gsPrice: c.gsPrice,
				lPrice: c.lPrice,
				price: c.price
			};
			return a;
		}, {});

	    var checkOOS = getStockStatus();
	    inks.forEach(function(ink, i) {
			ink.gsPrice = prices[ink.itemId].gsPrice;
			ink.lPrice = prices[ink.itemId].lPrice;
			ink.price = prices[ink.itemId].price;
			ink.isOOS = checkOOS(ink.sku);
			if ( ink.isOOS ) {
				ink.text = "OUT OF STOCK";
			} else {
				ink.text = "ADD TO CART";
			}
		});
	}

	function findMissingProds(needArr, hasArr) {
		var missingProds = [];
	    var hasObj = hasArr.reduce(function(a,c) {
	  		a[c.productId] = true;
			return a;
		}, {});
		needArr.forEach(function(prod) {
			if ( !hasObj[prod.itemId] ) missingProds.push(prod.itemId);
		});
		return missingProds;
	}

	/* get products inventory, and price data from HP Services */
	function getProductsData(itemids, cb){
		$.ajax(
		  	{url: HPSERVICES + encodeURIComponent(itemids), dataType: "json"}
		).done(function(ser){
			// push product data
			ser.priceData.forEach(function(v, i){
				$.hpsfData.getSvcData().priceData.push(ser.priceData[i]);
				$.hpsfData.getSvcData().inventoryData.push(ser.inventoryData[i]);
			});

		  	cb();
		}).error(function(e){
			hpUtil.log("wd-js","error", { 
				"id": "EOL Printer PDPs test - " + METRIC_VAR, 
				message: e
			});
		});
	}

	/* Check if product is oos */
	function getStockStatus(){ 
	    var skus = $.hpsfData.getSvcData().inventoryData;
	    var status = skus.reduce(function(a,c) {
	    	a[c.partNumber] = c.noStock;
	    	return a;
	    }, {});

	    return function(sku) { return status[sku]; };
	}

	function createCarousal() {
    	var target = $('#tabs_mobile .row.pdpBlock.scroll_reference');

		var slides = inks.reduce(function(a,c) {
			return a += hpUtil.templayed(carousalSlideHTML)(c);
		}, '');
		var carousal = hpUtil.templayed(carousalSectionHTML)({ title: 'Need ink for this printer?', slides: slides });
		target.after($(carousal));
		
		var linkToSlickFixed = '//store.hp.com/wcsstore/hpusstore/Treatment/slick-fixed.min.js';
		Bootstrapper.loadScriptCallback(linkToSlickFixed, function(){
			$('.eol-carousal').slick({
    			slidesToShow: 3,
    			slidesToScroll: 3,
    			infinite:false,
    			arrows: true,
    			fade: false,
    			responsive: [
	    			{
	      				breakpoint: 1125,
	      				settings: {
	        				dots:true,
	        				slidesToShow: 2,
	        				slidesToScroll: 2,
	        				arrows: false
	      				}
	    			},
	    			{
	      				breakpoint: 642,
	      				settings: {
	        				dots:true,
	        				slidesToShow: 1,
	        				slidesToScroll: 1,
	        				arrows: false
	      				}
	    			}
    			]
  			});
		});

        $('body').on('click', '.eol-add-to-cart:not(.isOOS)', function(){
        	var button = $(this);
        	button.addClass('busy');
			hpUtil.addToCart({
				itemId: this.getAttribute("data-itemId"),
				productId: this.getAttribute("data-prodId"),
			 	quantity: 1,
			 	cartMethod: "pdp"
        	});

        	// xsell tracking
        	var omni_products = [{
				'type': 'product',
				'sku': this.getAttribute("data-sku"),
				'units': '1',
				'price': this.getAttribute("data-regPrice"),
				'xsell': 'true'
        	}];
        	window.trackMetrics('new.page', {
		        promo: {
		          	location: METRIC_PREFIX + ':' + METRIC_VAR,
		          	parent_id: window.sku
		        }, 
		        product: {
		          	list: omni_products
		        }
	      	}); 

	      	setTimeout(function() {
	      		console.log('QA: finish tracking xsell');
	      		button.removeClass('busy');
	      	}, 2000);
        });
	}

	hpUtil.whenAvailable(function() {
		$ = window.$;
		var target = document.querySelector('.pdpPrice._desktop_only .inventory_holder .btn.bluebtn.text-center');
    	var targetM = document.querySelector('.priceHolderMbl._mobile_only .inventory_holder .btn.bluebtn.text-center');
		return $ && target && targetM;
	}).then(function(){
		try{
			initTest();
		}catch(e){
			hpUtil.log("wd-js","error",
			{ 
				"id": "EOL Printer PDPs test - " + METRIC_VAR, 
				message: e
			});
		}
	});

	hpUtil.whenAvailable(function() {
		$ = window.$;
    	return $ && $.hpsfData && $.hpsfData.getSvcData();
	}).then(function() {
		try{
			addInksModule();
		}catch(e){
			hpUtil.log("wd-js","error", { 
				"id": "EOL Printer PDPs test - " + METRIC_VAR, 
				message: e
			});
		}
