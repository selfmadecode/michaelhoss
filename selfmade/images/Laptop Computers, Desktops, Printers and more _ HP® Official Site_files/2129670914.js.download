  function StoreOptlyTracking() {
    var that = this;
    var orderComplete = false;
    var userTracked = false;
    var revenueTracked = false;
    var startT = new Date().getTime();
    window["optimizely"] = window["optimizely"] || [];
    var REVENUE_EVENT_PREFIX = {
      "category": "categoryRevenue_",
      "subcategory": "subcategoryRevenue_"
    };

    function releaseEvents(){
      var condition = ( !!window.adobeStCalled || (((new Date().getTime() - startT)  / 1000) > 40) );
      return condition;
    }

    // hold events on order confirm page
    window["optimizely"].push({
      "type": "holdEvents"
    });


    // wait till s.t has been called or it's been more than 40 seconds, and events are bucketed
    hpUtil.when(function(){
      return releaseEvents();
    }).then(function(){
      window["optimizely"].push({
        "type": "sendEvents"
      });
    });

    function trackOrderComplete(){
      if(!orderComplete){
        orderComplete = true;
        window['optimizely'].push({
          type: "event",
          eventName: "orderComplete"
        });
      }
    }

    function hashCode(str) {
      var hash = "0", i, chr;
      if (!str || str.length === 0) return null;
      try{
        for (i = 0; i < str.length; i++) {
          chr   = str.charCodeAt(i);
          hash  = ((hash << 5) - hash) + chr;
          hash |= 0; // Convert to 32bit integer
        }
        hash = Math.abs(hash).toString();
      }catch(e){

      }
      return hash;
    };

    function convertToCents(price){
      return Number(Math.round(parseFloat(price)+'e2')*100+'e-2');
    }

    function setLineItemTotals(event, order) {
      var catTotals = {};
      var prd;
      var catKey, subCatKey, prdPrice;

      try {
        //init aggs
        event.productRevenue = [];
        event.productSku = [];
        event.productName = [];
        event.productCategory = [];
        event.productSubcategory = [];
        event.productLine = [];
        event.productUnits = [];

        for (var li = 0; li < order.lineItems.length; li++) {
          prd = order.lineItems[li];
          catKey = REVENUE_EVENT_PREFIX.category + prd.mappedCategory;
          subCatKey = REVENUE_EVENT_PREFIX.subcategory + prd.mappedSubCategory;
          prdPrice = convertToCents(prd.price);

          catTotals[catKey] = (catTotals[catKey] || 0) + prdPrice;
          catTotals[subCatKey] = (catTotals[subCatKey] || 0) + prdPrice;

          if(subCatKey === "subcategoryRevenue_desktopconsumer" || subCatKey === "subcategoryRevenue_notebookconsumer"){
            catTotals["subcategoryRevenue_pcconsumer"] = (catTotals["subcategoryRevenue_pcconsumer"] || 0) + prdPrice;
          }
          if(subCatKey === "subcategoryRevenue_desktopcommercial" || subCatKey === "subcategoryRevenue_notebookcommercial"){
            catTotals["subcategoryRevenue_pccommercial"] = (catTotals["subcategoryRevenue_pccommercial"] || 0) + prdPrice;
          }

          event.productRevenue.push(convertToCents(prd.price));
          event.productSku.push(prd.sku);
          event.productName.push(prd.name);
          event.productCategory.push(prd.mappedCategory);
          event.productSubcategory.push(prd.mappedSubCategory);
          event.productLine.push(prd.productLine);
          event.productUnits.push(prd.qty);
        }
      } catch (e) {
        //pass
      }
      return {
        "event": event,
        "catTotals": catTotals
      };
    }

    function setOrderTracked(oid){
      try{
        window.sessionStorage[oid] = 1;
      }catch(e){
        window.hpUtil.setCookie(oid,1,.3);
      }
    }

    function isOrderTracked(oid){
      return (oid  in window.sessionStorage || !!window.hpUtil.getCookie(oid));
    }


    that.trackPage = function(name, tags) {
      if (!tags) {
        /*  configure in pages section perhaps, interested in:
            Site Section
            Mobile / Non-Mobile (matched with Omniture)
            Country (matched with Omniture)
         */
        return;
      }

      window["optimizely"].push({
        "type": "page",
        "pageName": name,
        "tags": tags
      });
    };

    that.trackUser = function(attr) {
      if (!attr && !userTracked) {
        //make default user tracking call
        !ORDER_CONFIRM && window.hpUtil.when(function(){
          return window.hpmmd && window.hpmmd.user && window.hpmmd.user.login && window.hpmmd.page && window.hpmmd.page.discount_tier && window.hpmmd.page.platform;
        }).then(function(){
          that.trackUser({
            "customerTier": window.hpmmd.page.discount_tier || "gen",
            "loginStatus": !window.hpmmd.user.login || window.hpmmd.user.login == "N" ? "N" : "Y",
            "userPlatform": window.hpmmd.page.platform,
          });

          userTracked = true;
        });
        return;
      }

      !ORDER_CONFIRM && window["optimizely"].push({
        "type": "user",
        "attributes": attr
      });
    };

    that.trackRevenue = function(tags, eventName) {
      //if no event data provided, set default revent tracking value
      if (!tags) {
        ORDER_CONFIRM && hpUtil.analytics.getOrder().then(function(o) {
          var oid = hashCode(o.id);
          //if this order has already been tracked this session do nothing
          if(!oid || isOrderTracked(oid)){
            hpUtil.log("wd-js", "info", {
              "id": "revenue",
              "message": "revenue de-dupped " + o.id + (window.sessionStorage ? " (session)" : " (cookie)")
            });
            return;
          }
          setOrderTracked(oid);
          trackOrderComplete();
          //build events for category and subcategory revenue 
          var event = {
            "revenue": convertToCents(o.subtotal),
            "orderId": o.id,
            "callCenter": o.callCenter,
            "paymentMethod": o.paymentMethod,
            "couponCode": o.coupon_code
          };
          //aggregate lineItem info
          var totals = setLineItemTotals(event, o);

          that.trackRevenue(totals.event, eventName);

          //track category revenue
          for(var ct in totals.catTotals){
            that.trackRevenue({
              "revenue": totals.catTotals[ct],
              "orderId": o.id
            }, ct);
          }
        });

        return;
      }

      window["optimizely"].push({
        "type": "event",
        "eventName": eventName || "trackRevenue",
        "tags": tags
      });

    };
