  function addPriceAndOffer() {
    // get price
    var lPrice = $('#price_value').text();
    var price = $('.price_strike');
    var innerHTML = '<span class="orange-price">$'+lPrice+'</span>';
    var eppPriceDiv = $('div.epp-price');
    if (!!price.length && Number(price[0].innerHTML.replace('$','')) > Number(lPrice)) {
      innerHTML += '  <span class="stricked">'+price[0].innerHTML+'</span>';
    }
    eppPriceDiv.html(innerHTML);
    // get user data
    if ($.hpsfData.getSvcData().profileData.personData.userType.toLowerCase().indexOf('epp') !== -1) {
    	$('span.eppp').html('EPP PRICE');
    }
    // get stock data
    var inventoryRecord = $.hpsfData.getSvcData().inventoryData.filter(function(e) { return e.partNumber === window.sku; })[0];
    if (inventoryRecord.lowStock && inventoryRecord.stock === 0) {
    	var buttonLink = $('.ojBtn.ojAddtoCartBtn');
    	buttonLink.removeClass('ojAddtoCartBtn');
    	buttonLink.addClass('ojOutofstockbtn');
    	buttonLink.html('OUT OF STOCK');
    } else if (inventoryRecord.lowStock) {
    	$('span.ojPurpleText.stock-message').html('Only ' + inventoryRecord.stock + ' left in stock');
    }
    // get offer
    var offer =  $('#scroll_offers').text().trim();
    $('span.splOffer').html(offer);

	$('span.splOffer').click(function(){
		if(document.documentElement.clientWidth>800) {
			$(".tabsUnderlined li a, .heroTabs li a").click.call($("a[title='specialoffertab']"));
		} else {
			$('#specialOffersForMobile button.accordion').click();
			$("html, body").animate({scrollTop:$("#specialOffersForMobile button.accordion").position().top+200},"slow");
		}
	});

	// get shipping message
  if ( !inventoryRecord.lowStock || !inventoryRecord.stock === 0 ) {
		var shippingMessage = 'Ships within 1 business day';
		if ( inventoryRecord.backOrderFlag ) shippingMessage = 'Ships by ' + inventoryRecord.invAvaDate;
		$('.ojPurpleText.free-shipping-message').after('<span class="ojPurpleText shipping-message">'+shippingMessage+'</span>');
	}

  $('.ojPurpleText.shipping-message').after('<a href="Add to cart" class="ojBtn ojAddtoCartBtn">Add to cart</a>');
	  
    // get itemId for Add to cart
    var productInfo = JSON.parse($('.priceBlock .pdpPrice .inventory_holder')[0].getAttribute("data-a2c"));
    var itemId1, itemId2, items = [];

    var carePacks = $('#carePacks li');
    var carePackSelect = $('.carePackMobile select');
    if (!!carePacks.length) {
      for ( var j = 1; j >= 0; j-- ) {
        var title = $('#carePacks li.main h3')[j].innerHTML;
        var price = $($('#carePacks li.main #price_value')[j]).text();
        var itemId = JSON.parse($('#carePacks li.main .inventory_holder')[j].getAttribute("data-omni")).itemId;
        
        var targetSpan = $($("span.chktext")[2-j]);
        targetSpan.prop('title',title);
        targetSpan.html(title+'<i>+$'+price+'</i>');
        var targetLi = $($(".ojCarePacks li")[2-j]);
        targetLi.css("display", "block");

        carePackSelect.append("<option value='"+(2-j)+"'>"+title+" ("+price+")</option>");

        items.push({ itemId: itemId, quantity: 1, price: price });
      }
    }

    var itemsToCart = [
      // { itemId: productItemId, quantity: 1 }
    ];
    var selections = $('ul.ojCarePacks li');
    function carePackClick() {
		var addVal = 0, subVal = 0;
		if ( itemsToCart.length === 1 ) {
			subVal = itemsToCart.pop().price;
		}
		if ( this.index > 0 ) {
			itemsToCart.push(items[this.index - 1]);
			addVal = items[this.index - 1].price;
		}
		updatePrice(addVal, subVal);
		$('.pdpfy17 .prdPrice .ojCarePacks input[type="radio"]#c'+(this.index+1)).prop( "checked", true );
		$('.carePackMobile select').prop('value',''+this.index);
    }
    function carePackChange() {
    	var index = carePackSelect[0].value;
    	selections[index].click();
    }

    for ( var i = 0; i < selections.length; i++ ) {
      selections[i].index = i;
      selections[i].addEventListener("click", carePackClick);
    }
    carePackSelect.change(carePackChange);


    var matchPrice = /[0-9]+(\.[0-9][0-9])?/g;
    function updatePrice(addVal, subVal) {
    	var oldHTML = eppPriceDiv.html();
    	var newHTML = oldHTML.replace(matchPrice, function(match, name, index) {
    		return '' + Math.round((Number(match) + Number(addVal) - Number(subVal)) * 100) / 100;
    	});
      	eppPriceDiv.html(newHTML);
    }

     var ATCBtn = document.querySelector('.ojAddtoCartBtn');
     var xsellShowed = false;
     if ( !!ATCBtn ) {
	     ATCBtn.addEventListener("click", function(event){
	       event.preventDefault();
	       if (!!itemsToCart.length) {
					
		       hpUtil.addToCartWCS(itemsToCart, null, true,function() {
			       hpUtil.addToCart({
			        itemId: productInfo.itemId,
			        productId: productInfo.prodId,
			        quantity: 1,
			        cartMethod: "pdp"
			       });
		       });
	       } else {
	       		hpUtil.addToCart({
	       		 itemId: productInfo.itemId,
	       		 productId: productInfo.prodId,
	       		 quantity: 1,
	       		 cartMethod: "pdp"
	       		});
	       }

	       if ( !xsellShowed ) {
		       hpUtil.whenAvailable(function() {
		       	return $(document).foundation;
		       }).then(function() {
		       	try {
		       		$('#xsell-modal').foundation('reveal', 'open');
		       		xsellShowed = true;
		       	}catch(e) {
		       		hpUtil.log("wd-js","error",
		       		           { 
		       		  "id": "AB Test OfficeJet Pro 8700 PDP", 
		       		  message: e
		       		});
		       	}
		       });
	       }
